<?xml version='1.0' encoding='utf-8'?>
<widget id="com.ypap.StudentPU" version="1.0.12" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Student PU</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="nandang.mulyadi@podomorouniversity.ac.id" href="http://podomorouniversity.ac.id">
        IT PU Team
    </author>
    <content src="template/login.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="StatusBarBackgroundColor" value="#004977" />
    <preference name="android-targetSdkVersion" value="28" />
</widget>
